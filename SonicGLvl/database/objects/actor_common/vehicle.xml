<?xml version="1.0" encoding="utf-8"?>
<Template>
  <type type="integer" default="4" description="1 - Jeep | 2 - Bike | 3 - Hovercraft | 4 - Glider" />
  <downfilename type="string" default="" description="Unknown." />
  <isGetOut type="bool" default="false" description="Determines if the character can exit the vehicle." />
  <isShoot type="bool" default="false" description="Determines if the vehicle has weapons." />
  <RoutePath type="string" default="" description="Name of the spline in a PATH file for the object to follow." />

  <Extra type="model" name="Gadget_Jeep.model" condition="type == 1" />
  <Extra type="model" name="Gadget_Bike.model" condition="type == 2" />
  <Extra type="model" name="Gadget_Hover.model" condition="type == 3" />
  <Extra type="model" name="Gadget_Glider.model" condition="type == 4" />
  <Extra type="description" name="I Am... All of Me." />
</Template>