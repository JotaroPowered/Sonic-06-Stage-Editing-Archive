  <Extra type="model" name="en_cTricker.model" condition="typeName == cTricker" />
  <Extra type="model" name="en_eArmor.model" condition="typeName == eArmor" />
  <Extra type="model" name="en_eBluster.model" condition="typeName == eBluster" />
  <Extra type="model" name="en_eBomber.model" condition="typeName == eBomber" />
  <Extra type="model" name="en_eBuster.model" condition="typeName == eBuster" />
