<?xml version="1.0" encoding="utf-8"?>
<Template>
  <count type="integer" default="0" description="Number of switches to be activated." />
  <target0 type="float" default="4294967295" description="First object to target." /> <!-- uint32 -->
  <target1 type="float" default="4294967295" description="Second object to target." /> <!-- uint32 -->
  <target2 type="float" default="4294967295" description="Third object to target." /> <!-- uint32 -->
  <target3 type="float" default="4294967295" description="Fourth object to target." /> <!-- uint32 -->
  <target4 type="float" default="4294967295" description="Fifth object to target." /> <!-- uint32 -->
  <target5 type="float" default="4294967295" description="Sixth object to target." /> <!-- uint32 -->
  <target6 type="float" default="4294967295" description="Seventh object to target." /> <!-- uint32 -->
  <target7 type="float" default="4294967295" description="Eighth object to target." /> <!-- uint32 -->
  <target8 type="float" default="4294967295" description="Ninth object to target." /> <!-- uint32 -->
  <event type="string" default="" description="Event to trigger when the object is enabled." />
  <DrawDistance type="float" default="0" description="How close the object needs to be to the player before spawning." />
  <ObjName type="string" default="" description="The internal name of the object." />
  <StartInactive type="bool" default="false" description="Whether the object starts inactive." />
  
  <Extra type="description" name="Collects multiple switches together to trigger an event." />
</Template>